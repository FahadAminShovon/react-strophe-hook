{
  "name": "react-strophe-hook",
  "version": "1.0.0",
  "description": "hook wrapper on top of strophe.js",
  "author": "FahadAminShovon",
  "license": "MIT",
  "repository": "FahadAminShovon/react-strophe-hook",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist/**/**"
  ],
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir dist",
    "start": "rollup -c -w",
    "prepare": "husky install",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "commit": "cz"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "strophe.js": "^1.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@commitlint/cli": "^16.1.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@types/strophe.js": "^1.2.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "commitizen": "^4.2.4",
    "commitlint-config-gitmoji": "^2.2.5",
    "cross-env": "^5.2.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^2.0.1",
    "husky": "^7.0.4",
    "prettier": "2.5.1",
    "react": "^16.9.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^2.26.9",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "strophe.js": "^1.4.3",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  }
}
